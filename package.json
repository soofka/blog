{
  "name": "@soofka/blog",
  "version": "1.4.0",
  "description": "soofka blog",
  "keywords": [
    "soofka",
    "blog",
    "react",
    "typescript"
  ],
  "author": "Jakub Sowi≈Ñski <pansoofka@gmail.com> (https://soofka.pl/)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/soofka/blog"
  },
  "engines": {
    "npm": ">=5.2",
    "node": ">=9"
  },
  "scripts": {
    "build": "npm run build:full",
    "build:full": "npm run build:full:prod",
    "build:full:dev": "npm run test && npm run build:clean && npm run build:dev",
    "build:full:prod": "npm run test && npm run build:clean && npm run build:prod",
    "build:dev": "cross-env env=dev npm run build:base",
    "build:prod": "cross-env env=prod npm run build:base",
    "build:base": "node ./scripts/builder/run.js",
    "build:clean": "rimraf ./dist",
    "start": "npm run start:dev",
    "start:dev": "cross-env env=dev node ./scripts/server/dev/run.js",
    "start:prod": "cross-env env=prod node ./scripts/server/dev/run.js",
    "test": "jest --config ./jestconfig.json",
    "test:coverage": "npm run test:clean && npm run test -- --coverage --no-cache",
    "test:update": "npm run test -- --updateSnapshot",
    "test:watch": "npm run test -- --watch",
    "test:clean": "rimraf ./coverage",
    "audit": "npm run build:clean && cross-env env=prod mode=audit npm run build:base",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:js": "eslint ./scripts/**/*.js",
    "lint:ts": "tslint ./src/**/*.{ts,tsx} --config ./tslintconfig.json",
    "lint:styles": "stylelint ./src/**/*",
    "clean": "npm run build:clean && npm run test:clean",
    "check-version": "node ./scripts/package/checkVersion.js",
    "postinstall": "npm run check-version"
  },
  "dependencies": {
    "axios": "^0.16.0",
    "babel-polyfill": "^6.26.0",
    "js-cookie": "^2.2.0",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "mobx-react-router": "^4.0.4",
    "query-string": "^6.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/enzyme": "^2.7.5",
    "@types/jest": "^18.1.1",
    "@types/react": "^16.1.0",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.4",
    "awesome-typescript-loader": "^5.2.0",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^6.24.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "body-parser": "^1.18.2",
    "compression-webpack-plugin": "^0.4.0",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.2.2",
    "eslint": "^4.17.0",
    "express": "^4.15.3",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "jest": "^23.5.0",
    "jest-styled-components": "^6.1.0",
    "raf": "^3.4.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "semver": "^5.5.0",
    "styled-components": "^3.4.5",
    "stylelint": "^8.4.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2",
    "stylelint-webpack-plugin": "^0.10.5",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^18.0.3",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-webpack-plugin": "^1.2.2",
    "typescript": "^2.7.1",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.0.1",
    "webpack-hot-middleware": "^2.21.2"
  }
}
