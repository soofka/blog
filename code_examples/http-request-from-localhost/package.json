{
  "name": "@soofka/blog/examples/http-request-from-localhost",
  "version": "1.0.0",
  "devDependencies": {
    "express": "^4.16.3",
    "http-proxy": "^1.17.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start:webpack:mock": "webpack-dev-server --config webpack.devserver.mock.config.js",
    "start:webpack:proxy": "webpack-dev-server --config webpack.devserver.proxy.config.js",
    "start:express:mock": "node express.mock.js",
    "start:express:proxy": "node express.proxy.js"
  }
}
